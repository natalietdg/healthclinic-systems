{
  "name": "capstone-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/crypto-js": "^4.0.2",
    "@types/radium": "^0.24.4",
    "@types/react": "^17.0.35",
    "@types/react-animations": "^1.0.0",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/testing-library__react": "^10.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-react-security": "^0.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "i18next": "^20.6.0",
    "jest": "^27.3.1",
    "module-alias": "^2.2.2",
    "path": "^0.12.7",
    "radium": "^0.26.1",
    "react": "^17.0.2",
    "react-animations": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-dotenv": "^0.1.3",
    "react-helmet-async": "^1.1.2",
    "react-i18next": "^11.11.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.6",
    "recoil": "^0.4.1",
    "sass": "^1.38.2",
    "sass-resources-loader": "^2.2.4",
    "serve": "^13.0.2",
    "typescript": "^4.4.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-dotenv && watch && webpack serve --host 0.0.0.0",
    "build": "webpack --config ./webpack.config.js && npx style-dictionary build",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "watch": "./node_modules/.bin/webpack --watch --config webpack.config.js",
    "serve": "serve -s build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.10",
    "@types/react-router": "^5.1.16",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^3.3.2",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.0",
    "json-minimizer-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.4.5",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.3.0",
    "style-dictionary": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.5.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.js"
    ],
    "verbose": true,
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "scss"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|less|css)$": "identity-obj-proxy",
      "^@root(.*)$": "<rootDir>/src$1",
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^Pages(.*)$": "<rootDir>/src/pages$1",
      "^Recoil(.*)$": "<rootDir>/src/recoil$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1",
      "^Services(.*)$": "<rootDir>/src/services$1",
      "^Public(.*)$": "<rootDir>/public$1",
      "^Shared(.*)$": "<rootDir>/src/components/shared$1",
      "^Helpers(.*)$": "<rootDir>/src/helpers$1",
      "^src(.*)$": "<rootDir>/src$1",
      "^Data(.*)$": "<rootDir>/src/data$1"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
